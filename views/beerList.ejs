<h2 class="text-center">Just check a beer</h2>


<input type="text" ng-model="beer" placeholder="Recherche"/>


<div style="border: solid 1px black">


    <div class="beer" ng-repeat="beer in beers | filter:beer" style="border: solid 1px black">
        <a href="/#/beer/{{ beer._id }}">{{ beer._id }}</a>
    </div>

</div>

